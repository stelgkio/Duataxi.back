  "jaeger": {
    "enabled": true,
    "serviceName": "coreapi",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "rabbitMq": {
    "namespace": "api",
    "retries": 1,
    "retryInterval": 1,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "rabbitmq"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "redis": {
    "connectionString": "localhost",
    "instance": ""
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "smtp-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5011"
      },
      {
        "name": "payment-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5002"
      },
