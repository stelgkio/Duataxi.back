      },
      {
        "name": "minibusapi-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5006"
      },
      {
        "name": "taxiapi-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5010"
      }
    ]
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": true,
    "url": "http://localhost:5341",
    "token": "secret"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
